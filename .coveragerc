# Copied from https://github.com/googleapis/synthtool/blob/master/synthtool/gcp/templates/python_library/.coveragerc
[run]
branch = True

[report]
fail_under = 70
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
    # Ignore abstract methods
    raise NotImplementedError
omit =
  */gapic/*.py
  */proto/*.py
  */core/*.py
  */site-packages/*.py
